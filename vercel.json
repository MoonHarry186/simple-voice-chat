{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/" }
  ],
  "headers": [
    {
      "source": "/files/css/(.*)", 
      "headers": [
        { "key": "Content-Type", "value": "text/css" }
      ]
    }
  ]
}
